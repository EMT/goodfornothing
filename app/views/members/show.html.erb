<section id="primary_content">
	<h1><%= @member.name %>'s profile for Nothing</h1>
	<% if user_signed_in? && current_user == @member %>
		<%= link_to "Edit", edit_user_registration_path %>
	<% end %>
	<% if @member.skills.any? %>
	<h2>A dab hand at:</h2>
	<ul>
		<% @member.skills.each do |skill| %>
		<li><%= skill.title %></li>
		<% end %>
	</ul>
	<% end %>
</section>