<section id="primary_content">
	<article class="post">
		<h1><%= @post.title %></h1>
		<time><%= @post.created_at.to_formatted_s(:long_ordinal) %></time>
		<%= markdown.render(@post.body).html_safe %>
	</article>
</section>

<aside id="secondary_content">
	<h2>Warblings</h2>
	<%= render :partial => "warblings/list" %>
</aside>