.section_registrations #pagination {
	margin: 0 auto;
	display: inline-block;
	padding: 0 0 0 15px;
	background: url(<%= asset_path "assets/white_squiggle.png" %>) repeat-x center center;
}

.section_registrations #pagination li.page_links {
	font-family: "Good for Nothing";
	display: inline-block;
	color: #ffffff;
	font-size: 18px;
	width: 30px;
	text-align: center;
	line-height: 36px;
	height: 31px;
	cursor: pointer;
	padding: 0;
	margin-right: 13px;
	background: url(<%= asset_path "assets/single_splodge_1.png" %>) no-repeat;
}

.section_registrations #pagination li.page_links:hover,
.section_registrations #pagination li.page_links:active,
.section_registrations #pagination li.page_links:focus,
.section_registrations #pagination li.page_links.current {
	background-position: left bottom;
	color: #ffffff;
}

.section_registrations #pagination .prev,
.section_registrations #pagination .next {
	text-indent: -13377px;
	display: block;
	height: 60px;
	width: 60px;
	position: absolute;
	top: 43%;
	left: 0;
	background: url(<%= asset_path "icons/previous.png" %>) no-repeat top left;
	cursor: pointer;
}

.section_registrations #pagination .next {
	right: 0; left: auto;
	background: url(<%= asset_path "icons/next.png" %>) no-repeat top left;
}

