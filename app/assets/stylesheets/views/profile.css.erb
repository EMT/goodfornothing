#profile_welcome {
	margin-bottom: 50px;
	width: 100%;
	overflow: hidden;
}

#profile_welcome h2 {
	text-align: center;
	margin-bottom: 45px;
}

#profile_welcome p {
	width: 540px;
	margin: 0 auto;
}

#profile_welcome ul {
	padding: 0;
	overflow: hidden;
	list-style-type: none;
	text-align: center;
	width: 560px;
	margin: 0 auto 5px;
}

#profile_welcome ul li {
	display: block;
	margin-right: 20px;
	float: left;
	width: 270px;
}

#profile_welcome ul li:last-child {
	margin: 0;
}

#profile .panel {
	background: #ffffff;
	padding: 20px;
	width: 410px;
}

#profile .panel #profile_avatar {
	display: block;
	width: 100%;
}

#profile .panel #profile_avatar .avatar {
}

#profile .panel ul {
	width: 100%;
	clear: both;
	display: block;
}